{
  "name": "profanitybuster",
  "version": "0.1.0",
  "description": "High-performance, TypeScript profanity detection and filtering library with modular language packs.",
  "keywords": [
    "profanity",
    "filter",
    "moderation",
    "typescript",
    "javascript",
    "content-moderation"
  ],
  "license": "MIT",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/tengfone/profanitybuster"
  },
  "bugs": {
    "url": "https://github.com/tengfone/profanitybuster/issues"
  },
  "author": "tengfone",
  "sideEffects": false,
  "engines": {
    "node": ">=20.19.0"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "peerDependencies": {
    "react": ">=17"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --sourcemap --clean --target es2020 --out-dir dist",
    "dev": "tsup src/index.ts --format cjs,esm --dts --sourcemap --clean --target es2020 --out-dir dist --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "vitest run",
    "bench": "vitest bench --run",
    "perf": "npm run bench",
    "prepublishOnly": "npm run build && npm run test"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/node": "^24.2.1",
    "@types/react": "^18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "tinybench": "^4.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
